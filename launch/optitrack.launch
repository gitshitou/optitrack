<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="server" value="192.168.1.12" />
    <arg name="local" value="0.0.0.0" />

    <node pkg="optitrack" type="optitrack_node" name="optitrack" output="screen">
        <param name="local" value="$(arg local)"/>
        <param name="server" value="$(arg server)"/>
    </node>

</launch>
